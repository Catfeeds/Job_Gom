  "main": "index.js",
  "scripts": {
    "clear": "rm -rf dist",
    "single": "npm run clear && cross-env NODE_ENV=development webpack -d --color --profile --config ./webpack.config.single.js",
    "multiple": "npm run clear && cross-env NODE_ENV=development webpack -d --color --profile --config ./webpack.config.template.js",
    "dist": "npm run clear && cross-env NODE_ENV=development webpack -d --color --profile --config ./webpack.config.js",
    "build": "npm run clear && cross-env NODE_ENV=production",
    "dev": "webpack-dev-server -d --color",
    "webserver": "webpack-dev-server -d --config ./webpack.config.server.js",
    "hotserver": "node server.js"
  },

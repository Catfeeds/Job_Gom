    "postcss-import": "^9.1.0",
    "postcss-loader": "^2.0.5",
    "rd": "^1.0.0",
    "sass-loader": "^6.0.5",
    "shelljs": "^0.7.7",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.0"
  },

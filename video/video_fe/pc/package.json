{
  "name": "gomeplus_video_pc",
  "version": "1.1.0",
  "description": "国美Plus-视频事业部PC站",
  "repository": "http://gitlab.intra.gomeplus.com/video-fe/pc.git",
  "scripts": {
    "view": "node ./config/bin/view.js",
    "new": "node ./config/bin/new.js",
    "remove": "node ./config/bin/remove.js",
    "lcdev": "node ./config/bin/dev.js --env=lcdev",
    "dev": "node ./config/bin/dev.js --env=dev",
    "pre": "node ./config/bin/dev.js --env=pre",
    "prd": "node ./config/bin/dev.js --env=production",
    "macdev": "authbind node ./config/bin/dev.js --env=dev",
    "build": "node ./config/bin/view.js && node ./config/bin/prod.js",
    "publish": "git subtree pull --prefix=config/webpack/flame http://code.ds.gome.com.cn/gitlab/video_fe/flame.git release --squash && git subtree push --prefix=config/webpack/flame http://code.ds.gome.com.cn/gitlab/video_fe/flame.git release",
    "changelog": "conventional-changelog -p angular -i mikelog.md -s -r 0",
    "sp": "git subtree pull --prefix=config/webpack/flame http://code.ds.gome.com.cn/gitlab/video_fe/flame.git release --squash",
    "nb": "git subtree split --rejoin --prefix=config/webpack/flame --branch newflame",
    "np": "git push http://code.ds.gome.com.cn/gitlab/video_fe/flame.git newflame:release"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.15.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",

/* pages/detail/detail.wxss */
video{
    width:100%;
    height:432rpx;
}
.detail-scroll{
    position:absolute;
    top:432rpx;
    bottom:0;
    background-color: #FFF;
}
.detail-video-info{
    padding: 20rpx;
    border-bottom: 1rpx solid #EEE;
}
.video-namebox{
    display: flex;
}
.detail-video-name{
    flex: 1;
    font-size: 36rpx;
    font-weight: bold;
    color: #333;
    line-height: 50rpx;
}
.detail-video-name text{
    padding: 10rpx 0;
}
.detail-video-desc{
    font-size: 28rpx;
    color: #999;
    line-height: 38rpx;
    margin: 20rpx 0;
}
.area-title{
    margin-top: 20rpx;
    font-size: 36rpx;
    font-weight: bold;
    color: #333;
    height: 40rpx;
    line-height: 40rpx;
    padding: 20rpx;
}
.related-videos{
    padding: 20rpx;
    overflow-x: scroll;
}
.related-item{
    width: 250rpx;
    float: left;
    margin-right: 20rpx;
}
.related-item-img{
    width: 100%;
    height: 140rpx;
}
.related-item-name{
    height: 40rpx;
    line-height: 40rpx;
    overflow: hidden;
    font-size: 36rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.related-item-desc{
    font-size: 28rpx;
    color: #999;
    line-height: 40rpx;
    height: 40rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}

.comments-list{
    padding: 0 20rpx 100rpx 20rpx;
    font-size: 28rpx;
}
.comments-item{
    border-top: 1rpx solid #eee;
    padding: 20rpx 0;
    display: flex;
}
.comments-left{
    width: 80rpx;
    padding-right: 20rpx;
}
.comments-right{
    flex:1;
}
.user-head{
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    line-height: 80rpx;
}
.user-name{
    height: 40rpx;
    line-height: 40rpx;
}
.comments-content{
    line-height: 38rpx;
    padding: 10rpx 0;
}
.comments-tool{
    height: 40rpx;
    line-height: 40rpx;
    overflow: hidden;
    color: #666;
}
.comments-time{
    float: left;
}
.comments-recom{
    width: 40rpx;
    height: 40rpx;
    background: url('data:image/png;base64,') no-repeat center;
    background-size: 28rpx 27rpx;
    float: right;
}
.comments-like{
    height: 40rpx;
    min-width: 60rpx;
    text-align: right;
    margin-left: 20rpx;
    float: right;
    padding-right: 40rpx;
    background: url('data:image/png;base64,') no-repeat right 8rpx;
    background-size: 28rpx 80rpx;
}
.comments-like.liked{
    background-position: right -44rpx;
}

.comment-bar{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #F30;
    font-size: 32rpx;
    box-shadow: 0 -2rpx 1rpx rgba(0,0,0,.3);
    background: #FFF;
}
.comment-form{
    position: fixed;
    left: 0;
    top:0;
    right: 0;
    bottom: 0;
    background: #eee;
    padding: 30rpx;
    z-index: 999;
}
.comment-form-title{
    padding:20rpx;
    height:40rpx;
    line-height:40rpx;
    font-size:32rpx;
    font-weight:bold;
}
.comment-textarea{
    padding: 20rpx;
    width: auto;
    height: 200rpx;
    font-size: 32rpx;
    line-height: 40rpx;
    color: #666;
    border-radius: 20rpx;
    background-color: #FFF;
}
.comment-btnbox{
    display: flex;
}
.comment-btn{
    flex: 1;
    margin:30rpx;
}
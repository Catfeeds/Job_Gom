	<div data-type="insertGoods" data-skuId="{{skuId}}" data-pid="{{PId}}" data-info='{"type":"item","text":"","id":"{{PId}}","kid":"", "shopId": "{{shopId}}","skuId":"{{skuId}}"}'  class="publish-item">
		<a href="{{link}}" class="img-out">
			<img src="{{img}}" data-node="goodsPic">

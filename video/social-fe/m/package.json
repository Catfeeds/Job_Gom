    "publish": "git subtree pull --prefix=config/webpack/flame-m http://code.ds.gome.com.cn/gitlab/social-fe/flame-m.git release --squash && git subtree push --prefix=config/webpack/flame-m http://code.ds.gome.com.cn/gitlab/social-fe/flame-m.git release",

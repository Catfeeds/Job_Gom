    "nb": "git subtree split --rejoin --prefix=config/webpack/flame-m --branch newflame",
    "np": "git push http://code.ds.gome.com.cn/gitlab/social-fe/flame-m.git newflame:release",
    "new": "node ./config/bin/new.js",
    "remove": "node ./config/bin/remove.js"
